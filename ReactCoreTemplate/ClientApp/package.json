{
  "name": "react-core-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "postbuild": "node postbuild.js --from=build --to=dist",
    "build:ssr": "react-app-rewired build --server",
    "postbuild:ssr": "node postbuild.js --from=build/bundle.js --to=dist/server/bundle.js --clean=dist/server",
    "build:all": "npm run scss & npm run build & npm run build:ssr",
    "postbuild:all": "node postbuild.js --clean=build",
    "scss": "node ./node_modules/typed-scss-modules/dist/lib/cli.js src/**/*.module.scss",
    "scss:watch": "node ./node_modules/typed-scss-modules/dist/lib/cli.js src/**/*.module.scss --watch",
    "eject": "react-app-rewired eject",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\""
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "https://localhost:5001",
  "devDependencies": {
    "@banbrick/redux-creator": "^1.0.8",
    "@types/classnames": "2.2.6",
    "@types/history": "4.7.0",
    "@types/node": "10.5.7",
    "@types/react": "16.8.3",
    "@types/react-dom": "16.8.1",
    "@types/react-helmet": "^5.0.7",
    "@types/react-redux": "7.0.1",
    "@types/react-router": "4.0.30",
    "@types/react-router-dom": "^4.3.0",
    "aspnet-prerendering": "^3.0.1",
    "fs-extra": "^7.0.1",
    "node-sass": "^4.11.0",
    "prettier": "^1.14.2",
    "react-app-rewire-aliases": "^0.2.0",
    "react-app-rewired": "^2.1.0",
    "react-hot-loader": "^4.6.5",
    "react-scripts": "^2.1.5",
    "reflect-metadata": "^0.1.12",
    "tslint": "^5.12.1",
    "typed-scss-modules": "0.0.6",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  }
}
